set -o allexport;
source .env;
set +o allexport;
envsubst < graph-node-configs/config.tmpl > graph-node-configs/config.toml;
envsubst < geo-configs/config.tmpl > geo-configs/config.yaml;
envsubst < nginx/nginx.conf.tmpl > nginx/nginx.conf;
envsubst < tap-agent/config.tmpl > tap-agent/config.toml;


export COMPOSE_HTTP_TIMEOUT=120
trap 'docker-compose -f compose-pinax.yml down' SIGINT SIGTERM
docker-compose -f compose-pinax.yml up --build $@ 2>&1 | tee pinax.log
